<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:gravity="center" 
    android:padding="30dp"
    android:background="@drawable/popup_window_round_corner_bg">
    <TextView 
        android:id="@+id/popup_rehearsal_result_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/popup_rehearsal_result_title_not_bad"
        android:textSize="30sp"
        />

    <TextView 
        android:id="@+id/popup_rehearsal_result_descrip"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text=""
        android:textSize="18sp"
        android:layout_marginTop="5dp"
        />
    <com.ef.bite.widget.RehearsalResultView
        android:id="@+id/popup_rehearsal_result_progress"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_marginTop="30dp"
        />
</LinearLayout>
